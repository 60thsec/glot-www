User
    email Text
    password Text
    created UTCTime
    modified UTCTime
    UniqueUser email
    deriving Typeable

Profile
    userId UserId
    snippetsApiId Text
    username Text
    name Text
    created UTCTime
    modified UTCTime
    UniqueProfile userId
    UniqueSnippetsApiId snippetsApiId
    UniqueUsername username
    deriving Typeable

ApiUser
    userId UserId
    snippetsId Text
    runId Text
    token Text
    created UTCTime
    modified UTCTime
    UniqueApiUser userId
    UniqueRunId runId
    UniqueSnippetsId snippetsId
    deriving Typeable

RunResult
    snippetId Text
    filesHash Text
    language Text
    stdout Text
    stderr Text
    error Text
    created UTCTime
    UniqueRunResult snippetId
    UniqueRunResultHash snippetId filesHash
    deriving Typeable

LanguageVersion
    snippetId Text
    version Text
    UniqueLanguageVersion snippetId
    deriving Typeable

RunCommand
    snippetId Text
    command Text
    UniqueRunCommand snippetId
    deriving Typeable

 -- By default this file is used in Model.hs (which is imported by Foundation.hs)
