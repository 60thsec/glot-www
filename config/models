User
    email Text
    password Text
    created UTCTime
    modified UTCTime
    UniqueUser email
    deriving Typeable

ApiUser
    userId UserId
    snippetsId Text
    runId Text
    token Text
    created UTCTime
    modified UTCTime
    UniqueRunId runId
    UniqueSnippetsId snippetsId
    deriving Typeable

