#editor { 
    height: 800px;
}

.editor-widget .expand {
    float: right;
}

.editor-widget .compress {
    float: right;
}

.close-file {
    margin-left: 10px;
    font-size: 12px;
    line-height: 22px;
    color: red;
    opacity: 0.4;
}

.close-file:hover {
    opacity: 1;
    cursor: pointer;
}
