$newline never
<nav>
  <ul class="pagination">
    $maybe prevPage <- paginationPrevPage pagination
      <li>
        <a href="@?{(route, [("page", prevPage)])}" aria-label="Previous">
          <span aria-hidden="true">--
    $nothing
      <li class="disabled">
        <span aria-label="Previous">
          <span aria-hidden="true">--
    <li class="disabled">
      <span>page = #{currentPage}
    $maybe nextPage <- paginationNextPage pagination
      <li>
        <a href="@?{(route, [("page", nextPage)])}" aria-label="Next">
          <span aria-hidden="true">++
    $nothing
      <li class="disabled">
        <span aria-label="Next">
          <span aria-hidden="true">++
